{
  "name": "main-site",
  "version": "1.0.0",
  "private": true,
  "description": "hollowverse",
  "author": "M.K. Safi",
  "scripts": {
    "dev-no-ssl": "cross-env DEV_SERVER_URL='http://localhost:3000' next dev",
    "dev": "cross-env DEV_SSL_SERVER_URL='https://localhost:3000' run-p 'dev-no-ssl -- -p 2999' 'local-ssl-proxy --source 3000 --target 2999'",
    "build": "run-s 'symlink-shared' 'next build'",
    "symlink-shared": "SYMLINK_DESTINATION=main-site node ../symlink-shared.js",
    "start": "run-s 'build' 'next start'",
    "export": "next export",
    "next": "next",
    "test": "jest --watch",
    "lint": "next lint",
    "tsc": "tsc",
    "lighthouse": "lighthouse-batch -f ${URL_FILE:-lighthouse/local.txt} --html --no-report --params '--only-categories accessibility,best-practices,performance,seo'; open report/lighthouse",
    "e2e": "playwright test",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "local-ssl-proxy": "local-ssl-proxy --source 3000 --target 2999",
    "unused-exports": "ts-unused-exports ./tsconfig.json --excludePathsFromReport=pages"
  },
  "dependencies": {
    "@google-analytics/data": "^2.8.0",
    "@logtail/browser": "^0.1.12",
