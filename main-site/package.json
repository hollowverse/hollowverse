{
  "name": "main-site",
  "version": "1.0.0",
  "private": true,
  "description": "hollowverse",
  "author": "M.K. Safi",
  "scripts": {
    "dev": "cross-env DEV_SERVER_URL='http://localhost:3000' next dev",
    "dev-ssl": "cross-env DEV_SSL_SERVER_URL='https://localhost:3000' run-p 'dev -- -p 2999' 'local-ssl-proxy --source 3000 --target 2999'",
    "build": "next build",
    "start": "next build && next start",
    "export": "next export",
    "next": "next",
    "test": "jest --watch",
    "lint": "next lint",
    "lighthouse": "lighthouse-batch -f ${URL_FILE:-lighthouse/local.txt} --html --no-report --params '--only-categories accessibility,best-practices,performance,seo'; open report/lighthouse",
    "lighthouse-single": "cross-env URL_FILE='lighthouse/single-local.txt' npm run lighthouse",
    "lighthouse-live": "cross-env URL_FILE='lighthouse/live.txt' npm run lighthouse",
    "manual-validation": "cross-env URL_FILE='lighthouse/local.txt' playwright test",
    "manual-validation-live": "cross-env URL_FILE='lighthouse/live.txt' playwright test",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "local-ssl-proxy": "local-ssl-proxy --source 3000 --target 2999",
    "postinstall": "SYMLINK_DESTINATION=main-site node ../symlink-shared.js"
  },
  "dependencies": {
    "@google-analytics/data": "^2.8.0",
    "@logtail/browser": "^0.1.12",
    "@logtail/js": "^0.1.12",
    "@logtail/node": "^0.1.12",
    "@next/bundle-analyzer": "^12.1.2",
    "@playwright/test": "^1.21.0",
    "@sanity/client": "^3.2.2",
    "@sanity/image-url": "^1.0.1",
    "@sanity/webhook": "^1.0.2",
    "@sentry/cli": "^2.0.4",
    "@sentry/nextjs": "^6.19.7",
    "@svgr/webpack": "^6.1.2",
    "@types/cors": "^2.8.12",
    "@types/fast-levenshtein": "^0.0.2",
    "@types/jest": "^27.5.0",
    "@types/lodash-es": "^4.17.6",
    "@types/open-graph-scraper": "^4.8.1",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.4",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "delay": "^5.0.0",
    "detect-browser": "^5.3.0",
    "eslint": "8.5.0",
